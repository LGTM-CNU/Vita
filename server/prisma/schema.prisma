generator client {
  provider = "prisma-client-js"
}

// 데이터 베이스 연동
datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

// 데이터 베이스 스키마
model User {
  id Int @default(autoincrement()) @id
  
}

model Medicine {
  id Int @default(autoincrement()) @id
  name String
}

model Todo {
  id Int @default(autoincrement()) @id
}